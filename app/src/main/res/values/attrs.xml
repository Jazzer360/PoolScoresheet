<?xml version="1.0" encoding="utf-8"?>
<resources>

	<declare-styleable name="PlayerScoreView">
		<attr name="game" format="integer" />
		<attr name="round" format="integer" />
		<attr name="breaks" format="boolean" />
	</declare-styleable>
	<declare-styleable name="SumView">
		<attr name="isCircled" format="boolean" />
		<attr name="sumChangeAnimation">
			<enum name="none" value="1" />
			<enum name="flip" value="2" />
		</attr>
		<attr name="sumRule">
			<enum name="always" value="1" />
			<enum name="gray_when_missing" value="2" />
			<enum name="not_when_missing" value="3" />
		</attr>
	</declare-styleable>
	<declare-styleable name="IntegerView">
		<attr name="mustSum" format="boolean" />
	</declare-styleable>

</resources>